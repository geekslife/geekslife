			KeyCode::KEY_1,ModifierFlag::SHIFT_L|ModifierFlag::COMMAND_L|ModifierFlag::OPTION_L|ModifierFlag::CONTROL_L
			, KeyCode::VK_CONSUMERKEY_BRIGHTNESS_DOWN
